#%RAML 1.0 DataType
type: object

properties:
  ModuleType:
    required: true  
    type: number
    description: "This field determines type of email template. Possible values."
  Id:
    required: true
    type: string
    description: "Id holds the InvoiceId of respective invoice"
  Recipients:
    required: true
    type: array
    description: "List of recipient email addresses."


